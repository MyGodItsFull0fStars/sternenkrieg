<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rebelartstudios.sternenkrieg">
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <!--Needed for Google Nearby Connections                                  -->
        <meta-data android:name="com.google.android.gms.nearby.connection.SERVICE_ID"
            android:value="@string/service_id" />

        <activity android:name="com.example.rebelartstudios.sternenkrieg.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.rebelartstudios.sternenkrieg.Spielen"
            android:label="@string/app_name" />
        <activity
            android:name="com.example.rebelartstudios.sternenkrieg.Modi"
            android:label="@string/app_name" />
        <activity
            android:name="com.example.rebelartstudios.sternenkrieg.Wuerfeltest"
            android:label="@string/app_name" />
        <activity
            android:name="com.example.rebelartstudios.sternenkrieg.EndScreen"
            android:label="@string/app_name" />
        <activity
            android:name="com.example.rebelartstudios.sternenkrieg.Map"
            android:label="@string/app_name" />
        <activity
            android:name="com.example.rebelartstudios.sternenkrieg.Networking"
            android:label="@string/app_name" />
        <activity
            android:name="com.example.rebelartstudios.sternenkrieg.Spielfeld"
            android:label="SK_UI3" />
        <activity android:name="com.example.rebelartstudios.sternenkrieg.Options" />
        <activity android:name="com.example.rebelartstudios.sternenkrieg.About" />
        <activity android:name="com.example.rebelartstudios.sternenkrieg.Socket.Client" />
        <activity android:name="com.example.rebelartstudios.sternenkrieg.Socket.Servers" />
        <activity android:name="com.example.rebelartstudios.sternenkrieg.Socket.Socket_main" />

        <service android:name="com.example.rebelartstudios.sternenkrieg.PlayAudio" android:enabled="true" />
    </application>

</manifest>